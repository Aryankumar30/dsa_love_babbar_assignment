using stl

#include <iostream>
#include <stack>
using namespace std;

int main()
{
   stack<int> st;
   st.push(5);
   st.push(25);
   st.push(53);
   st.push(51);
   cout<<st.size()<<endl;
   cout<<st.empty()<<endl;
   cout<<st.top()<<endl;
st.pop();
 cout<<st.top();

    return 0;
}

