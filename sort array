#include <iostream>

using namespace std;
//slection sort
int main()
{
    int arr[10] = {1,6,2,9,4,0,-3};
    int n = 7;
    
    for(int i = 0 ; i < n ; i ++){
         for(int j = i+1 ; j<n ;j++){
             if(arr[i] > arr[j]){
                 int temp = arr[i];
                 arr[i] = arr[j];
                 arr[j] = temp;
             }
         }
    }
    
    for(int i = 0 ; i< n ; i++){
        cout<<arr[i]<<" ";
    }
    return 0;
}

