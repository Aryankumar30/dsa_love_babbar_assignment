#include <iostream>

using namespace std;
//slection sort
int main()
{
    int arr[10] = {1,6,2,9,4,0,-3};
    int n = 7;
    
    for(int i = 0 ; i < n ; i ++){
         for(int j = i+1 ; j<n ;j++){
             if(arr[i] > arr[j]){
                 int temp = arr[i];
                 arr[i] = arr[j];
                 arr[j] = temp;
             }
         }
    }
    
    for(int i = 0 ; i< n ; i++){
        cout<<arr[i]<<" ";
    }
    return 0;
}

------------------------------

#include <iostream>

using namespace std;
// bubble sort we can add break statent wgen no swap happen at first to make best case o(n)
int main()
{
    int arr[10] = {1, 6, 2, 9, 4, 0, -3};
    int n = 7;

    for (int i = 0; i < n - 1; i++)
    {

        for (int j = 0; j < n - i - 1; j++)
        {
            if (arr[j] > arr[j + 1])
            {
                int temp = arr[j + 1];
                arr[j + 1] = arr[j];
                arr[j] = temp;
            }
        }
    }

    for (int i = 0; i < n; i++)
    {
        cout << arr[i] << " ";
    }
    return 0;
}


